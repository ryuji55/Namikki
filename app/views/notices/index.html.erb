<div class="container">
  <div class="col-lg-10 offset-lg-1">
    <h1 class="title">Namikki</h1>
  </div>

  <div class="board-list jscroll">
    <div class="row">
      <% notices = @notices.where.not(visitor_id: current_user.id) %>
      <% if notices.exists? %>
        <%= render notices %>
      <% else %>
        <p> 通知はありません </p>
      <% end %>
      <%= paginate notices %>
    </div>
  </div>
</div>
